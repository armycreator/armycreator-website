{% extends 'SitiowebArmyCreatorBundle::layout.html.twig' %}

{% block h1 %}Game creation{% endblock %}

{% block content %}
<form action="{{ path('admin_game_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">{{ "button.create"|trans }}</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_game') }}">
            {{ "default.back_to_list"|trans }}
        </a>
    </li>
</ul>
{% endblock %}
